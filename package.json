{
  "name": "DatInstaller",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "prepare": "patch-package",
    "build-back": "./build-back.sh",
    "build-front": "tsc -p ./tsconfig-frontend.json",
    "prestart": "npm run build-back && npm run build-front",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "update-android-version": "react-native-version --target android --never-amend",
    "license-check": "license-check",
    "test": "jest"
  },
  "license-check-config": {
    "src": [
      "src/**/*.ts",
      "index.android.js",
      "!src/**/*.d.ts",
      "!./node_modules/**/*"
    ],
    "path": "license-banner.txt",
    "blocking": false,
    "logInfo": false,
    "logError": true
  },
  "dependencies": {
    "@cycle/http": "^14.5.0",
    "@cycle/isolate": "^3.1.0",
    "@cycle/run": "^3.3.0",
    "@types/express": "^4.0.37",
    "@types/node": "^8.0.34",
    "@types/react": "^16.0.10",
    "@types/react-native": "^0.49.2",
    "body-parser": "^1.18.2",
    "cycle-native-navigation": "^1.1.0",
    "cycle-onionify": "^4.0.0",
    "express": "^4.16.2",
    "noderify": "^3.0.2",
    "patch-package": "^3.5.1",
    "react": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-native-action-button": "^2.8.0",
    "react-native-apk-installer": "^0.0.2",
    "react-native-fs": "^2.8.1",
    "react-native-navigation": "^1.1.485",
    "react-native-node": "3.0.x",
    "react-native-progress": "^3.4.0",
    "react-native-simple-markdown": "^1.0.60-rc.3",
    "rxjs": "^5.4.3",
    "typescript": "^2.5.3",
    "xstream": "^11.0.0"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "21.2.1",
    "license-check": "^1.1.5",
    "mkdirp": "^0.5.1",
    "react-native-version": "^2.3.1",
    "react-test-renderer": "16.0.0-beta.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
